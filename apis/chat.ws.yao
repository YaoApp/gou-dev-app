{
  "name": "A Chat WebSocket server",
  "description": "A Chat WebSocket serverr",
  "version": "0.9.2",
  "protocols": ["yao-chat-01"],
  "guard": "bearer-jwt",
  "buffer": { "read": 1024, "write": 1024 },
  "limit": { "read-wait": 10, "pong-wait": 20, "max-message": 1024 },
  "timeout": 5,
  "process": "flows.websocket.chat"
}

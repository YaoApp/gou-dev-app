{
  "name": "Basic",
  "version": "1.0.0",
  "description": "Basic",
  "nodes": [
    {
      "name": "categories",
      "process": "models.category.Get",
      "args": [
        {
          "select": ["id", "name"],
          "limit": 3,
          "orders": [{ "column": "created_at", "option": "desc" }]
        }
      ]
    },
    {
      "name": "users",
      "process": "models.user.Paginate",
      "args": [
        {
          "select": ["id", "name", "mobile"],
          "orders": [{ "column": "created_at", "option": "desc" }],
          "wheres": [{ "column": "status", "value": "enabled" }]
        },
        1,
        3
      ],
      "outs": ["{{$out.data}}"]
    },
    {
      "name": "query",
      "engine": "query-test",
      "query": {
        "select": ["id", "name"],
        "from": "$user",
        "wheres": [
          { ":status": "status", "=": "enabled" },
          { ":created_at": "Created date", ">=": "?:$in[0]" },
          { ":created_at": "Created date", "<=": "?:$in[1]" }
        ],
        "limit": 3
      },
      "outs": ["{{$out}}"]
    }
  ],
  "output": {
    "params": "{{$in}}",
    "data": {
      "categories": "{{$res.categories}}",
      "users": "{{$res.users[0]}}",
      "query": "{{$res.query[0]}}"
    }
  }
}
